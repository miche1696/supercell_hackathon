# Copy to .env and set your key
OPENAI_KEY=your_openai_api_key_here
OPENAI_IMAGE_MODEL=chatgpt-image-latest
OPENAI_IMAGE_SIZE=1024x1024
OPENAI_IMAGE_QUALITY=medium

# Optional demo progression mode (skips LLM-generated boundaries/rules progression actions)
DEMO=0
DEMO_PROGRESS_PATH=backend/demo_progression.json

# Optional tracing config
TRACE_ENABLED=1
TRACE_STDOUT=1
TRACE_FILE=logs/trace.ndjson
